<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Hypertrophy.Pages.FoodDetailsPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="FoodName" />
            <Label x:Name="FoodCalories" />
            <Label x:Name="FoodProtein" />
            <Label x:Name="FoodFat" />
            <Label x:Name="FoodCarb" />
            <Button x:Name="DeleteFood" Text="Delete Food" Clicked="DeleteFood_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>